local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "._.",
	LoadingTitle = "Welcome to ._. (paid)",
	LoadingSubtitle = "by 3qxt#9919",
	ConfigurationSaving = {
		Enabled = true,
		FileName = "._."
	},
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "._.",
		Subtitle = "Key System",
		Note = "Put in the key that you got from the bot when bought",
		Key = "welcometo._."
	}
})
Rayfield:Notify("Welcome to ._.","Thanks for buying",10010348543) -- (t,c,image)

local HomeTab = Window:CreateTab("Home")
local ReTab = Window:CreateTab("Reanimations")
